        {
          "Host": "basket.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/basket/BasketCheckOut",
      "UpstreamHttpMethod": [
        "POST"
      ],
      //"AuthenticationProviderKey": "IdentityApiKey",
      //"AllowedScopes": []
    },
    {
      "DownstreamPathTemplate": "/basket/wallet/{username}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
