    {
      "DownstreamPathTemplate": "/api/Order/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "Ordering.api",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/order/{id}",
      "UpstreamHttpMethod": [
        "DELETE"
      ],
      //"AuthenticationProviderKey": "IdentityApiKey",
      //"AllowedScopes": []
